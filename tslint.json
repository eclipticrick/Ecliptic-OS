{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "max-line-length": [true, 140],
        "semicolon": false,
        "no-console": false,
        "trailing-comma": false,
        "ordered-imports": false,
        "object-literal-key-quotes": false,
        "object-literal-sort-keys": false,
        "arrow-parens": false,
        "curly": false
    },
    "rulesDirectory": []
}
